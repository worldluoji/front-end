{
    "root": true,
    "env": {
        "node": true
    },
    "extends": [
        "plugin:vue/recommended",
        "@vue/eslint-config-prettier"
    ],
    "rules": {
        "vue/require-default-prop": "off",
        "no-console": "off",
        "vue/v-bind-style": "off",
        "vue/v-on-style": "off",
        "no-unused-vars": "off",
        "vue/no-v-html": "off"
    },
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "globals": {
        "location": true,
        "document": true,
        "window": true,
        "GLOBAL": true
    }
}